{
  "name": "portfolio-client",
  "version": "1.0.0",
  "type": "module",
  "description": "Modern portfolio website showcasing full-stack development skills with React, Vite, and Tailwind CSS",
  "author": "Cheekati Sanjay Goud <sanjaycheekati83@gmail.com>",
  "license": "MIT",
  "homepage": "https://sanjaycheekati.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/SanjayCheekati/portfolio.git"
  },
  "keywords": [
    "portfolio",
    "react",
    "vite",
    "tailwind",
    "framer-motion",
    "full-stack",
    "developer"
  ],
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "node scripts/generate-sitemap.js && node scripts/generate-static-pages.js",
    "preview": "vite preview",
    "lint": "eslint src --ext js,jsx",
    "perf:measure": "npm run build && lighthouse http://localhost:4173 --output json --output html --output-path ./performance/lighthouse-report --chrome-flags=\"--headless\" --preset=desktop & npm run preview",
    "perf:check": "node performance/check-performance.js",
    "perf:optimize": "npm run optimize-images && npm run extract-critical && npm run precompress",
    "optimize-images": "node scripts/optimize-images.js",
    "extract-critical": "node scripts/extract-critical-css.js",
    "precompress": "node scripts/precompress.js",
    "generate-sw": "node scripts/generate-sw.js"
  },
  "dependencies": {
    "@nextui-org/react": "^2.6.11",
    "@vercel/analytics": "^1.6.1",
    "axios": "1.4.0",
    "framer-motion": "^12.23.25",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.2.1",
    "resend": "^6.5.2",
    "tailwind-variants": "^3.2.2"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.14",
    "critters": "^0.0.24",
    "glob": "^11.0.0",
    "lighthouse": "^12.2.1",
    "postcss": "8.4.24",
    "sharp": "^0.33.5",
    "tailwindcss": "3.4.1",
    "vite": "5.2.0",
    "workbox-build": "^7.3.0",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  }
}
